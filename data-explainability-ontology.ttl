@prefix : <http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/> .

<http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology> rdf:type owl:Ontology ;
                                                                                             owl:versionIRI <http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/0.1> ;
                                                                                             <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
                                                                                             rdfs:comment "This is an ontology covering data explainability and related concepts that improve or adviseray to it."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/contributesTo
:contributesTo rdf:type owl:ObjectProperty ;
               owl:inverseOf :isRealizedBy ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Characterization
                                           :DataExplainability
                                           :Governance
                                           :Methodology
                                           :Transparency
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :DataExplainability
                                          :Fairness
                                          :Transparency
                                          :Trust
                                        )
                          ] ;
               rdfs:comment "This property is used whenever A contributes something to B."@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/dictates
:dictates rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :guides ;
          owl:inverseOf :isDictatedBy ;
          rdf:type owl:IrreflexiveProperty ;
          rdfs:domain :Privacy ;
          rdfs:range :PrivacyPreservingDataAnalysis ;
          rdfs:comment "Dictates is a special form of guides that is stronger making the guidelines mandatory"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/employs
:employs rdf:type owl:ObjectProperty ;
         owl:inverseOf :isEmployedBy ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :ExploratoryDataAnalysis
                                     :FeatureEngineering
                                     :Visualization
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :DimensonalityReduction
                                    :Visualization
                                  )
                    ] ;
         rdfs:comment "This property is used whenever A uses and therfore employs B"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/guides
:guides rdf:type owl:ObjectProperty ;
        owl:inverseOf :isGuidedBy ;
        rdf:type owl:IrreflexiveProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Governance
                                    :HumanInvolvment
                                  )
                    ] ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Characterization
                                   :Methodology
                                 )
                   ] ;
        rdfs:comment "This property is used when A provides guidelines for B"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/hinders
:hinders rdf:type owl:ObjectProperty ;
         owl:inverseOf :isHinderedBy ;
         rdf:type owl:IrreflexiveProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Anomalies
                                     :Biases
                                     :Privacy
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :DataExplainability
                                    :ExplainableEDA
                                    :Transparency
                                    :Trust
                                  )
                    ] ;
         rdfs:comment "This propery is used whenever A makes it more difficult to achieve B."@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/identifies
:identifies rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :produces ;
            owl:inverseOf :isIdentifiedBy ;
            rdf:type owl:IrreflexiveProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :DatasetDescription
                                        :ExploratoryDataAnalysis
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Anomalies
                                       :Biases
                                     )
                       ] ;
            rdfs:comment "This property is used when A makes B discoverable pointing directly at it"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isDictatedBy
:isDictatedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isGuidedBy ;
              rdfs:domain :PrivacyPreservingDataAnalysis ;
              rdfs:range :Privacy ;
              rdfs:comment "The reverse of dictates"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isEmployedBy
:isEmployedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :DimensonalityReduction
                                          :Visualization
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :ExploratoryDataAnalysis
                                         :FeatureEngineering
                                         :Visualization
                                       )
                         ] ;
              rdfs:comment "The reverse of employs" .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isGuidedBy
:isGuidedBy rdf:type owl:ObjectProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Characterization
                                        :Methodology
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Governance
                                       :HumanInvolvment
                                     )
                       ] ;
            rdfs:comment "The reverse of guides"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isHinderedBy
:isHinderedBy rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :DataExplainability
                                          :ExplainableEDA
                                          :Transparency
                                          :Trust
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Anomalies
                                         :Biases
                                         :Privacy
                                       )
                         ] ;
              rdfs:comment "The reverse of hinders"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isIdentifiedBy
:isIdentifiedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isProducedBy ;
                rdf:type owl:IrreflexiveProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Anomalies
                                            :Biases
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :DatasetDescription
                                           :ExploratoryDataAnalysis
                                         )
                           ] ;
                rdfs:comment "The reverse of idetnfies"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isMitigatedBy
:isMitigatedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :mitigates ;
               rdf:type owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Anomalies
                                           :Biases
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :DataManipulation
                                          :DatasetDescription
                                        )
                          ] ;
               rdfs:comment "The reverse of mitigates"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isPrecededBy
:isPrecededBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :precedes ;
              rdf:type owl:TransitiveProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :DataManipulation
                                          :FeatureEngineering
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :DataManipulation
                                         :DataMining
                                         :ExploratoryDataAnalysis
                                       )
                         ] ;
              rdfs:comment "The reverse of precedes" .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isProducedBy
:isProducedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :produces ;
              rdf:type owl:IrreflexiveProperty ;
              rdfs:domain :Characterization ;
              rdfs:range :Methodology ;
              rdfs:comment "Meaning the domain is a physical result of the domain, not something abstract"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isRealizedBy
:isRealizedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :DataExplainability
                                          :Fairness
                                          :Transparency
                                          :Trust
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Characterization
                                         :DataExplainability
                                         :Governance
                                         :Methodology
                                         :Transparency
                                       )
                         ] ;
              rdfs:comment """Meaning the domain is a more abstract result of the domain, not something actual physical, e.g. both explainability and trust are abstract concepts
In contrast isProducedBy points to more phyiscal results so something that can acutally be used""" .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/mitigates
:mitigates rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :DataManipulation
                                       :DatasetDescription
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Anomalies
                                      :Biases
                                    )
                      ] .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/precedes
:precedes rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :DataManipulation
                                      :DataMining
                                      :ExploratoryDataAnalysis
                                    )
                      ] ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :DataManipulation
                                     :FeatureEngineering
                                   )
                     ] ;
          rdfs:comment "This property is used when A comes before B"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/produces
:produces rdf:type owl:ObjectProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :Methodology ;
          rdfs:range :Characterization ;
          rdfs:comment "This property is used when A produces something physical B"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology#hasCreationDate
:hasCreationDate rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :DataProvenance ;
                 rdfs:range xsd:dateTimeStamp .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology#hasFileFormat
:hasFileFormat rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :DatasetDescription ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology#hasMean
:hasMean rdf:type owl:DatatypeProperty ;
         rdfs:domain :StatisticalCharacterisation ;
         rdfs:range xsd:float .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology#hasMedian
:hasMedian rdf:type owl:DatatypeProperty ;
           rdfs:domain :StatisticalCharacterisation ;
           rdfs:range xsd:float .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology#hasStandardDeviation
:hasStandardDeviation rdf:type owl:DatatypeProperty ;
                      rdfs:domain :StatisticalCharacterisation ;
                      rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Accesible
:Accesible rdf:type owl:Class ;
           rdfs:subClassOf :FAIR ;
           rdfs:comment """Once the user finds the required data, they need to know how they can be accessed.
Therefore, data should be retrievable by their identifier using a standardized communications protocol, which is open, free and universally implementable and allows for an authentication and authorization procedure, if necessary.
Additionally, the metadata should stay available even if the data are no longer available."""@en ;
           rdfs:label "Accesible"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Accountability
:Accountability rdf:type owl:Class ;
                rdfs:subClassOf :Governance ;
                rdfs:comment "Accountability is about the responsibility of individuals or organizations for their actions regarding data"@en ;
                rdfs:label "Accountability"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/AdversarialBias
:AdversarialBias rdf:type owl:Class ;
                 rdfs:subClassOf :Biases ;
                 rdfs:comment "Adversial bias is bias that is introduced on purpose, often done with malicious intent."@en ;
                 rdfs:label "Adversarial Bias"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Anomalies
:Anomalies rdf:type owl:Class ;
           rdfs:subClassOf :Characterization ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hinders ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :DataExplainability
                                                               :ExplainableEDA
                                                               :Trust
                                                             )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :isIdentifiedBy ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :DatasetDescription
                                                               :ExploratoryDataAnalysis
                                                             )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :isMitigatedBy ;
                             owl:allValuesFrom :DataManipulation
                           ] ;
           rdfs:comment "Anomalies are problems or unexpected occurrences in the data. They can be actually wrong but also might be an actual occurence in the underlying process"@en ;
           rdfs:label "Anomalies"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Biases
:Biases rdf:type owl:Class ;
        rdfs:subClassOf :Characterization ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hinders ;
                          owl:allValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( :DataExplainability
                                                            :ExplainableEDA
                                                            :Trust
                                                          )
                                            ]
                        ] ;
        owl:disjointWith :Fairness ;
        rdfs:comment "Biases are influencing the data, mostly in a negative way. These can be brought on by the society, data collection as well as handling and methods used on the data."@en ;
        rdfs:label "Biases"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Characterization
:Characterization rdf:type owl:Class ;
                  rdfs:comment "Elements that help characterize data, like findings of analysis and other kinds of artifacts."@en ;
                  rdfs:label "Characterization"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Clustering
:Clustering rdf:type owl:Class ;
            rdfs:subClassOf :DatasetSummarization ;
            rdfs:comment "Clustering bundles data points that are similiar in a way using only a single one mostly the center of the cluster" ;
            rdfs:label "Clustering"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Context-WiseAnomalies
:Context-WiseAnomalies rdf:type owl:Class ;
                       rdfs:subClassOf :Anomalies ;
                       rdfs:comment "Context-wise anomalies are larger groups of point that deviate from the expected behavior"@en ;
                       rdfs:label "Context-Wise Anomalies"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataAudits
:DataAudits rdf:type owl:Class ;
            rdfs:subClassOf :Methodology ;
            rdfs:comment "They can provide important information about metadata."@en ;
            rdfs:label "Data Audits"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataCards
:DataCards rdf:type owl:Class ;
           rdfs:subClassOf :DatasetDescription ;
           rdfs:comment "Data Cards are structured summaries of essential facts about various aspects of datasets needed by stakeholders across a project's lifecycle."@en ;
           rdfs:label "Data Cards"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataCleaning
:DataCleaning rdf:type owl:Class ;
              rdfs:subClassOf :DataManipulation ;
              rdfs:comment "Data cleaning describes the process of removing faulty or wrong data instances"@en ;
              rdfs:label "Data Cleaning"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataExplainability
:DataExplainability rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :contributesTo ;
                                      owl:allValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( :Fairness
                                                                        :Trust
                                                                      )
                                                        ]
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :isRealizedBy ;
                                      owl:allValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( :Characterization
                                                                        :Governance
                                                                        :Methodology
                                                                        :Transparency
                                                                      )
                                                        ]
                                    ] ;
                    rdfs:comment "Data Explainbility, is the idea that all aspects of the data should be explainable, this includes all aspects from governance, methodology and characterization."@en ;
                    rdfs:label "Data Explainability"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataImputation
:DataImputation rdf:type owl:Class ;
                rdfs:subClassOf :DataManipulation ;
                rdfs:comment "Data imputation is the process of fixing or adding missing data points by using the information of the other data points"@en ;
                rdfs:label "Data Imputation"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation
:DataManipulation rdf:type owl:Class ;
                  rdfs:subClassOf :Methodology ;
                  rdfs:comment "Data manipulation are any methods that change the data in some way"@en ;
                  rdfs:label "Data Manipulation"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataMining
:DataMining rdf:type owl:Class ;
            rdfs:subClassOf :DataManipulation ;
            rdfs:comment "Data mining is the process of finding new features in an existing data set."@en ;
            rdfs:label "Data Mining"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataProvenance
:DataProvenance rdf:type owl:Class ;
                rdfs:subClassOf :Governance ;
                rdfs:comment "Data provenance refers to the origin and history of the data, including how it was collected, processed, and transformed"@en ;
                rdfs:label "Data Provenance"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataSheets
:DataSheets rdf:type owl:Class ;
            rdfs:subClassOf :DatasetDescription ;
            rdfs:comment "Data sheets are similar to data cards, but additionally to them also contain more information of for example the orign, recommended uses etc."@en ;
            rdfs:label "Data Sheets"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataSquashing
:DataSquashing rdf:type owl:Class ;
               rdfs:subClassOf :DatasetSummarization ;
               rdfs:comment "Data squasing collapses multiple data points into one, using only the information of that single point"@en ;
               rdfs:label "Data Squashing"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DatasetDescription
:DatasetDescription rdf:type owl:Class ;
                    rdfs:subClassOf :Characterization ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :mitigates ;
                                      owl:allValuesFrom :Biases
                                    ] ;
                    rdfs:comment "Dataset description are any aspects that give more context to the data"@en ;
                    rdfs:label "Dataset Description"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DatasetSummarization
:DatasetSummarization rdf:type owl:Class ;
                      rdfs:subClassOf :Methodology ;
                      rdfs:comment "Dataset summarization are methods that somehow make the information of the dataset available in a quick overview"@en ;
                      rdfs:label "Dataset Summarization"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DimensonalityReduction
:DimensonalityReduction rdf:type owl:Class ;
                        rdfs:subClassOf :DataManipulation ;
                        rdfs:comment "Dimensionality reduction is about reducing the dimension, so features of a data set" ;
                        rdfs:label "Dimensonality Reduction"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DomainSpecific
:DomainSpecific rdf:type owl:Class ;
                rdfs:subClassOf :FeatureEngineering ;
                owl:disjointWith :ModelBased .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DomainSpecificFeatureCreation
:DomainSpecificFeatureCreation rdf:type owl:Class ;
                               owl:equivalentClass [ owl:intersectionOf ( :DomainSpecific
                                                                          :FeatureCreation
                                                                        ) ;
                                                     rdf:type owl:Class
                                                   ] ;
                               rdfs:subClassOf :FeatureEngineering .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DomainSpecificFeatureExtraction
:DomainSpecificFeatureExtraction rdf:type owl:Class ;
                                 owl:equivalentClass [ owl:intersectionOf ( :DomainSpecific
                                                                            :FeatureExtraction
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ] ;
                                 rdfs:subClassOf :FeatureEngineering .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DomainSpecificFeatureSelection
:DomainSpecificFeatureSelection rdf:type owl:Class ;
                                owl:equivalentClass [ owl:intersectionOf ( :DomainSpecific
                                                                           :FeatureSelection
                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ] ;
                                rdfs:subClassOf :FeatureEngineering .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/ExplainableEDA
:ExplainableEDA rdf:type owl:Class ;
                rdfs:subClassOf :ExploratoryDataAnalysis ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isHinderedBy ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( :Biases
                                                                    :Privacy
                                                                  )
                                                    ]
                                ] ;
                rdfs:comment "Explainable EDA is EDA, that only employs inherently explainable methods."@en ;
                rdfs:label "Explainable EDA"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/ExploratoryDataAnalysis
:ExploratoryDataAnalysis rdf:type owl:Class ;
                         rdfs:subClassOf :Methodology ;
                         rdfs:comment "Exploratory data analysis, is the process of analysing the data to gain knowledge of the datasets features, like correlation, trends and other statistics"@en ;
                         rdfs:label "Exploratory Data Analysis"@en .


###  http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/FAIR
:FAIR rdf:type owl:Class ;
      rdfs:subClassOf :Governance ;
