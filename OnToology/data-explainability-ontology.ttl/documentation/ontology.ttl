###  https://w3id.org/def/data-explainbility-ontology/contributesTo
<https://w3id.org/def/data-explainbility-ontology/contributesTo> rdf:type owl:ObjectProperty ;
                                                                 owl:inverseOf <https://w3id.org/def/data-explainbility-ontology/isRealizedBy> ;
                                                                 rdf:type owl:TransitiveProperty ;
                                                                 rdfs:domain [ rdf:type owl:Class ;
                                                                               owl:unionOf ( <https://w3id.org/def/data-explainbility-ontology/Characterization>
                                                                                             <https://w3id.org/def/data-explainbility-ontology/DataExplainability>
                                                                                             <https://w3id.org/def/data-explainbility-ontology/Governance>
                                                                                             <https://w3id.org/def/data-explainbility-ontology/Methodology>
                                                                                             <https://w3id.org/def/data-explainbility-ontology/Transparency>
                                                                                           )
                                                                             ] ;
                                                                 rdfs:range [ rdf:type owl:Class ;
                                                                              owl:unionOf ( <https://w3id.org/def/data-explainbility-ontology/DataExplainability>
                                                                                            <https://w3id.org/def/data-explainbility-ontology/Fairness>
                                                                                            <https://w3id.org/def/data-explainbility-ontology/Transparency>
                                                                                            <https://w3id.org/def/data-explainbility-ontology/Trust>
                                                                                          )
                                                                            ] ;
                                                                 rdfs:comment "This property is used whenever A contributes something to B."@en ;
                                                                 rdfs:label "contributes to"@en .


###  https://w3id.org/def/data-explainbility-ontology/dictates
<https://w3id.org/def/data-explainbility-ontology/dictates> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <https://w3id.org/def/data-explainbility-ontology/guides> ;
                                                            owl:inverseOf <https://w3id.org/def/data-explainbility-ontology/isDictatedBy> ;
                                                            rdf:type owl:IrreflexiveProperty ;
                                                            rdfs:domain <https://w3id.org/def/data-explainbility-ontology/Privacy> ;
                                                            rdfs:range <https://w3id.org/def/data-explainbility-ontology/PrivacyPreservingDataAnalysis> ;
                                                            rdfs:comment "Dictates is a special form of guides that is stronger making the guidelines mandatory"@en ;
                                                            rdfs:label "dictates"@en .


