    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Accesible -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Accesible">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/FAIR"/>
        <rdfs:comment xml:lang="en">Once the user finds the required data, they need to know how they can be accessed.
Therefore, data should be retrievable by their identifier using a standardized communications protocol, which is open, free and universally implementable and allows for an authentication and authorization procedure, if necessary.
Additionally, the metadata should stay available even if the data are no longer available.</rdfs:comment>
        <rdfs:label xml:lang="en">Accesible</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Accountability -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Accountability">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Governance"/>
        <rdfs:comment xml:lang="en">Accountability is about the responsibility of individuals or organizations for their actions regarding data</rdfs:comment>
        <rdfs:label xml:lang="en">Accountability</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/AdversarialBias -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/AdversarialBias">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Biases"/>
        <rdfs:comment xml:lang="en">Adversial bias is bias that is introduced on purpose, often done with malicious intent.</rdfs:comment>
        <rdfs:label xml:lang="en">Adversarial Bias</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Anomalies -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Anomalies">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Characterization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/hinders"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataExplainability"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/ExplainableEDA"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Trust"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isIdentifiedBy"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DatasetDescription"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/ExploratoryDataAnalysis"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isMitigatedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Anomalies are problems or unexpected occurrences in the data. They can be actually wrong but also might be an actual occurence in the underlying process</rdfs:comment>
        <rdfs:label xml:lang="en">Anomalies</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Biases -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Biases">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Characterization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/hinders"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataExplainability"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/ExplainableEDA"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Trust"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Fairness"/>
        <rdfs:comment xml:lang="en">Biases are influencing the data, mostly in a negative way. These can be brought on by the society, data collection as well as handling and methods used on the data.</rdfs:comment>
        <rdfs:label xml:lang="en">Biases</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Characterization -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Characterization">
        <rdfs:comment xml:lang="en">Elements that help characterize data, like findings of analysis and other kinds of artifacts.</rdfs:comment>
        <rdfs:label xml:lang="en">Characterization</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Clustering -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Clustering">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DatasetSummarization"/>
        <rdfs:comment>Clustering bundles data points that are similiar in a way using only a single one mostly the center of the cluster</rdfs:comment>
        <rdfs:label xml:lang="en">Clustering</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Context-WiseAnomalies -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Context-WiseAnomalies">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Anomalies"/>
        <rdfs:comment xml:lang="en">Context-wise anomalies are larger groups of point that deviate from the expected behavior</rdfs:comment>
        <rdfs:label xml:lang="en">Context-Wise Anomalies</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataAudits -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataAudits">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Methodology"/>
        <rdfs:comment xml:lang="en">They can provide important information about metadata.</rdfs:comment>
        <rdfs:label xml:lang="en">Data Audits</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataCards -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataCards">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DatasetDescription"/>
        <rdfs:comment xml:lang="en">Data Cards are structured summaries of essential facts about various aspects of datasets needed by stakeholders across a project&apos;s lifecycle.</rdfs:comment>
        <rdfs:label xml:lang="en">Data Cards</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataCleaning -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataCleaning">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation"/>
        <rdfs:comment xml:lang="en">Data cleaning describes the process of removing faulty or wrong data instances</rdfs:comment>
        <rdfs:label xml:lang="en">Data Cleaning</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataExplainability -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataExplainability">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/contributesTo"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Fairness"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Trust"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/isRealizedBy"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Characterization"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Governance"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Methodology"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Transparency"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Data Explainbility, is the idea that all aspects of the data should be explainable, this includes all aspects from governance, methodology and characterization.</rdfs:comment>
        <rdfs:label xml:lang="en">Data Explainability</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataImputation -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataImputation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation"/>
        <rdfs:comment xml:lang="en">Data imputation is the process of fixing or adding missing data points by using the information of the other data points</rdfs:comment>
        <rdfs:label xml:lang="en">Data Imputation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Methodology"/>
        <rdfs:comment xml:lang="en">Data manipulation are any methods that change the data in some way</rdfs:comment>
        <rdfs:label xml:lang="en">Data Manipulation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataMining -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataMining">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataManipulation"/>
        <rdfs:comment xml:lang="en">Data mining is the process of finding new features in an existing data set.</rdfs:comment>
        <rdfs:label xml:lang="en">Data Mining</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataProvenance -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataProvenance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/Governance"/>
        <rdfs:comment xml:lang="en">Data provenance refers to the origin and history of the data, including how it was collected, processed, and transformed</rdfs:comment>
        <rdfs:label xml:lang="en">Data Provenance</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataSheets -->


    <owl:Class rdf:about="http://www.semanticweb.org/christianroters/ontologies/2025/7/data-explainability-ontology/DataSheets">
